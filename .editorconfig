# Editor configuration file

# End .editorconfig scan at this directory level
root = true

[**]
charset = utf-8
indent_size = 4
tab_width = 4
insert_final_newline = true
trim_trailing_whitespace = true

[*.{c,cpp,h,hpp}]
indent_style = tab

[*.vcxproj*]
insert_final_newline = false
indent_size = 2
tab_width = 2
indent_style = space
